<html>
<head>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/skeleton/2.0.4/skeleton.min.css" type="css">
  <link href='https://fonts.googleapis.com/css?family=Source+Code+Pro:700' rel='stylesheet' type='text/css'>
  <link rel="stylesheet" href="style.css" type="css">
</head>
<body>

  <div class="container">
    <div class="row">
      <div class="twelve columns">
        <center>
          <h1>SCOREBOARD</h1>
        </center>
      </div>
    </div>

    <div class="row">
      <div class="twelve columns">
      <center>
        <table class="u-half-width">
          <thead>
            <tr>
              <th>Name</th>
              <th>Correct</th>
              <th>Incorrect</th>
              <th>Percent Correct</th>
            </tr>
          </thead>
          <tbody>
            {{#people}}
            <tr>
            <td>{{name}}</td>
            <td>{{correct}}</td>
            <td>{{incorrect}}</td>
            <td>{{percent correct}}</td>
            </tr>
            {{/people}}
          </tbody>
        </table>
      </center>
      </div>
    </div>
  </div>

</body>
</html>
